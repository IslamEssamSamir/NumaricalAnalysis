@model List<Bisection>
<div class="card-body">
   
    <table class="table table-hover table-bordered">
        <thead>
            <tr>
                <th> # </th>
                <th> Xl </th>
                <th> f(Xl) </th>
                <th> Xu </th>
                <th> f(Xu) </th>
                <th> Xr </th>
                <th> f(Xr) </th>
                <th> error </th>
            </tr>
        </thead>


        @foreach (var row in Model)
        {
            <tbody>
                <tr>
                    <td>@row.Iteration</td>
                    <td>@row.Xl</td>
                    <td>@row.F_Xl</td>
                    <td>@row.Xu</td>
                    <td>@row.F_Xu</td>
                    <td>@row.Xr</td>
                    <td>@row.F_Xr</td>
                    <td>@row.Error %</td>
                    
                    
                </tr>
            </tbody>
        }


       
    </table>
</div>